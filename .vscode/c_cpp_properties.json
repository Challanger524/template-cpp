{
  "configurations": [
    {
      "name": "config",
      "includePath": [
        "${workspaceFolder}/include/**",
        "${workspaceFolder}/src/**"
      ],
      "forcedInclude": [
        "${workspaceFolder}/src/config.hpp"
      ],
      "cStandard":     "c23",
      "cppStandard": "c++23",
      "compilerArgs": [
        "/Zc:__cplusplus"
      ],
      "configurationProvider": "ms-vscode.cmake-tools"
    }
  ],
  "version": 4
}
